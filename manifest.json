{
  "name": "DemoIUH",
  "description": "My First Chrome Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/jquery.min.js","js/md5.js","js/java.js"],
      "run_at" : "document_start"
    }
  ],
   "web_accessible_resources": [
    "js/jquery.min.js","js/md5.js","js/java.js"
  ],
  "permissions": [
     "cookies",
    "*://sv.iuh.edu.vn/*"
  ],
  "manifest_version": 2
}